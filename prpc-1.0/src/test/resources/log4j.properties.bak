log4j.rootLogger=debug
log4j.logger.com.github.weaponlin=debug,prpc
log4j.appender.prpc=org.apache.log4j.ConsoleAppender
log4j.appender.prpc.Threshold=DEBUG
log4j.appender.prpc.ImmediateFlush=true
log4j.appender.prpc.Target=System.out
log4j.appender.prpc.layout=org.apache.log4j.PatternLayout
log4j.appender.prpc.layout.ConversionPattern=[%-5p] %d(%r) --> [%t] %l: %m %x %n